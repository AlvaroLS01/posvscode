				</textElement>
				<text><![CDATA[1. AVISO LEGAL]]></text>
			</staticText>
			<staticText>
				<reportElement uuid="d9abd0ee-0531-454e-88dc-8b6102cb5b61" x="-1" y="376" width="555" height="14"/>
