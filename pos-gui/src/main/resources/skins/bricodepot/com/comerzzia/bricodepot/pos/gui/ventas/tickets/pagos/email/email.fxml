            <Label fx:id="lbTitulo" alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" text="%Introduzca su email" />
          </children>
        </HBox>
        <VBox alignment="CENTER" prefHeight="103.0" prefWidth="-1.0">
          <children>
            <HBox alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0">
